# Token du bot Discord (OBLIGATOIRE)
# Obtenez-le sur https://discord.com/developers/applications
DISCORD_TOKEN=votre_token_ici

# ID du serveur Discord (OBLIGATOIRE pour les commandes)
# Clic droit sur ton serveur > Copier l'identifiant du serveur
GUILD_ID=votre_guild_id_ici

# ID du channel Discord pour les logs (0 = logs en console uniquement)
LOG_CHANNEL_ID=0

# ID du channel par défaut pour poster les giveaways (0 = channel obligatoire dans la commande)
DEFAULT_GIVEAWAY_CHANNEL_ID=0

# ID du rôle autorisé à créer des giveaways (0 = seulement les administrateurs)
# Vous pouvez mettre plusieurs rôles séparés par des virgules: 123456789,987654321
GIVEAWAY_ROLE_ID=0
